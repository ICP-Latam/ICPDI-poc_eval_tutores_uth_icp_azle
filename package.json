{
    "scripts": {
        "dfx_install": "DFX_VERSION=0.15.0 sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
        "replica_start": "dfx start --background",
        "replica_stop": "dfx stop",
        "canister_deploy_local": "dfx deploy eval_tutores_prueba",
        "canister_deploy_mainnet": "dfx deploy --network ic eval_tutores_prueba",
        "canister_uninstall": "dfx canister uninstall-code eval_tutores_prueba",
        "canister_call_get_message": "dfx canister call eval_tutores_prueba getMessage",
        "canister_call_set_message": "dfx canister call eval_tutores_prueba setMessage '(\"Hello world!\")'"
    },
    "dependencies": {
        "azle": "0.18.2"
    }
}

